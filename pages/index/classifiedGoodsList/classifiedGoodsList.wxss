page {
  background-color: rgb(240, 240, 240);
}

.selector_wrap {
  background-color: white;
  display: flex;
  font-size: 32rpx;
  height: 60rpx;
  line-height:60rpx;
  color: rgb(70, 70, 70);
}

.selector1 {
  width: 50%;
  text-align: center;
  border: 1rpx solid rgb(202, 202, 202);
  height: 60rpx;
  line-height: 60rpx;
}

.selector2 {
  width: 50%;
  text-align: center;
  border: 1rpx solid rgb(202, 202, 202);
height: 60rpx;
  line-height: 60rpx;
}

.goods_list_wrap {
  /* background-color: rgba(240, 240, 240, 0.39); */
  border-radius: 20rpx;
  margin: 0 auto;
  width: 700rpx;
  /* background-color: yellow; *//* border: 1px solid yellowgreen; */
  display: flex;
  flex-wrap: wrap;
}

.goods_list {
  display: block;
  margin: 20rpx 0 0 33rpx;
  width: 300rpx;
  /* min-width: 350rpx; */
  height: 400rpx;
  background: white;
  border-radius: 15rpx;
  overflow: hidden;
}

.goods_image {
  width: 300rpx;
  height: 250rpx;
}

.goods_detail {
  /* font-size: 30rpx; */
  padding: 0 0 10rpx 0rpx;
  text-align: center;
}

.goods_detail .goods_title {
  width: 300rpx;
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 34rpx;
}

.goods_detail .goods_price {
  font-size: 26rpx;
  color: rgba(255, 39, 24, 0.753);
  font-weight: bold;
  text-align: center;
}

.goods_detail  .likeamount {
  font-size: 22rpx;
  color: rgb(192, 192, 192);
  margin: 5rpx;
}
