.homePage-background {
  width: 100%;
  height: 1500rpx;
  padding: 60rpx 0 0 0;
  background: -webkit-linear-gradient(-90deg,rgb(255, 164, 137),#ffc697);
  background-color: #ffd4bb;
}

.homePage-personalInformation {
  margin: 0 30rpx 0 20rpx;
  /* border: 1px solid rgb(255, 253, 234); *//* background-color: rgba(255, 255, 255, 0.2); */
  display: flex;
  /* background-color: yellow; */
}

.homePage-personalInformation image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  border: 4rpx solid white;
  /* display: inline-block; */
  box-flex: 0;
}

.homePage-personalInformation-detail {
  box-flex: 0;
  /* background-color: red; */
}

.homePage-personalInformation .username {
  font-size: 48rpx;
  color: rgb(247, 253, 255);
  width: 470rpx;
  height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 20rpx 0 0 20rpx;
  /* display:inline-block; */
}

.tinysize {
  color: rgb(255, 255, 255);
  font-size: 30rpx;
  margin: 0rpx 0 0 20rpx;
  /* display: inline-block; */
}

.homePage-personalCenter {
  margin: 50rpx auto;
  width: 700rpx;
  height: 180rpx;
  background-color: white;
  border-radius: 20rpx;
  /* box-shadow: 0rpx 1rpx 2rpx 2rpx  #ccc; */

}

.homePage-personalCenter-iconView-view {
  /* margin: 30rpx 0; */
  position: relative;
  bottom: -25rpx;
  display: flex;
  text-align: center;

  /* padding: 50rpx 0 0 25rpx; *//* border-bottom: 1rpx solid rgb(255, 153, 0); */
}

.homePage-personalCenter-iconView {
  width: 160rpx;
  margin: 500rpx 0 0 0ï¼›;

}

.homePage-personalCenter-iconView image {
  height: 70rpx;
  width: 70rpx;
  position: relative;
  right: -25rpx;
}

.homePage-personalCenter-iconView text {
  font-size: 30rpx;
  color: rgb(255, 153, 0);
  /* text-align: center; */
  position: relative;
  right: -25rpx;
}

.newgoods_wrap {
  width: 700rpx;
  height: 260rpx;
  background-color: white;
  border-radius: 20rpx;
  margin: 0 auto;
  /* box-shadow: 0rpx 1rpx 2rpx 2rpx  #ccc; */
 

}

.newgoods_wrap navigator {
  margin: 30rpx auto;
  width: 700rpx;
}

.homePage-personalCenter-newGoods {
  /* margin: 200rpx; */
  display: block;
  width: 300rpx;
  height: 300rpx;
  text-align: center;
  /* border: 2rpx dashed rgb(255, 153, 0); */
  margin: 30rpx auto;
  color: rgb(255, 153, 0);
}

.homePage-personalCenter-newGoods image {
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx auto 0 auto;
  display: block;
  /* opacity: 0.7; */
}

